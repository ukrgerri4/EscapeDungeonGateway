{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Default": "Information",
        "System": "Information",
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
          "restrictedToMinimumLevel": "Debug"
        }
      }
    ]
  },
  "Cors": {
    "Origins": [
    ]
  },
  "IdentityServer": {
    "AuthorityUrl": "http://localhost:5000"
  }
}
