{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Default": "Information",
        "System": "Information",
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
          "restrictedToMinimumLevel": "Debug"
        }
      }
      //,{
      //  "Name": "File",
      //  "Args": {
      //    "path": "logs/log.txt",
      //    "rollingInterval": "Day",
      //    "restrictedToMinimumLevel": "Warning"
      //  }
      //}
    ]
  },
  "Cors": {
    "Origins": [
      "http://localhost:6010",
      "http://localhost:6007"
    ]
  },
  "IdentityServer": {
    "AuthorityUrl": "http://localhost:5000"
  }
}
